{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "baseUrl": ".",
    "lib": [
      "es2020",
      "dom"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "paths": {
      "@constructs": [
        "lib/constructs"
      ],
      "@constructs/*": [
        "lib/constructs/*"
      ],
      "@stacks": [
        "lib/stacks"
      ],
      "@stacks/*": [
        "lib/stacks/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}